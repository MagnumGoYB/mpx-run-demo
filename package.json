{
  "private": true,
  "name": "mpx-run-demo",
  "version": "0.1.0",
  "scripts": {
    "build": "mpx-cli-service build --targets=wx,web,tt",
    "build:h5": "mpx-cli-service build --targets=web",
    "build:tt": "mpx-cli-service build --targets=tt",
    "build:wx": "mpx-cli-service build --targets=wx",
    "commit": "git-cz",
    "format": "prettier . --write --log-level warn && prettier-package-json --write",
    "preinstall": "only-allow pnpm",
    "lint": "eslint --ext .js,.ts,.mpx src/",
    "prepare": "husky || true",
    "serve": "mpx-cli-service serve --targets=wx,web,tt",
    "serve:h5": "mpx-cli-service serve --targets=web",
    "serve:tt": "mpx-cli-service serve --targets=tt",
    "serve:wx": "mpx-cli-service serve --targets=wx"
  },
  "dependencies": {
    "@mpxjs/api-proxy": "^2.10.0",
    "@mpxjs/core": "^2.10.0",
    "@mpxjs/fetch": "^2.10.0",
    "@mpxjs/store": "^2.10.0",
    "@mpxjs/utils": "^2.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/runtime-corejs3": "^7.10.4",
    "@commitlint/cli": "^19.8.0",
    "@mpxjs/babel-plugin-inject-page-events": "^2.9.0",
    "@mpxjs/eslint-config": "^2.0.3",
    "@mpxjs/mpx-cli-service": "^2.0.0",
    "@mpxjs/size-report": "^2.10.0",
    "@mpxjs/vue-cli-plugin-mpx": "^2.0.0",
    "@mpxjs/vue-cli-plugin-mpx-eslint": "^2.0.0",
    "@mpxjs/vue-cli-plugin-mpx-typescript": "^2.0.0",
    "@mpxjs/vue-cli-plugin-mpx-web": "^1.6.30",
    "@mpxjs/webpack-plugin": "^2.10.0",
    "@total-typescript/ts-reset": "^0.6.1",
    "@vue/cli-service": "~5.0.0",
    "autoprefixer": "^10.2.4",
    "commitlint-config-gitmoji": "^2.3.1",
    "eslint": "^9.0.0",
    "git-cz": "^4.9.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "only-allow": "^1.2.1",
    "postcss": "^8.2.6",
    "prettier": "^3.3.3",
    "prettier-package-json": "^2.8.0",
    "pretty-quick": "^4.0.0",
    "process": "^0.11.10",
    "rimraf": "^6.0.1",
    "sass-loader": "^16.0.5",
    "stylus-loader": "^8.1.2",
    "typescript": "^5.7.2",
    "vue": "2.7.16",
    "vue-demi": "^0.14.6",
    "vue-i18n": "8.27.2",
    "vue-i18n-bridge": "^9.2.2",
    "vue-router": "^3.1.3",
    "webpack": "^5.43.0"
  },
  "engines": {
    "node": "20.x"
  },
  "browserslist": [
    "ios >= 8",
    "chrome >= 47"
  ],
  "packageManager": "pnpm@10.8.1"
}
